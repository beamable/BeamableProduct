        { "include": [ ".gitignore", ".dockerignore" ] },
        { "exclude": [ ".vs/**", ".template_config/**", "[Content_Types].xml" ,"_rels/**"] }
      ]
    }
  ],
  "symbols": {
    "TemplateVersion": {
      "replaces": "TemplateVersion",
