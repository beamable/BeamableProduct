{
  "$schema": "http://json.schemastore.org/template",
  "author": "Beamable",
  "classifications": [ "beamable", "portal", "extension" ],
  "tags": {
    "language": "JavaScript",
    "type": "project"
  },
  "identity": "Beamable.Templates.PortalExtensionApp",
  "name": "Beamable Portal Extension",
  "shortName": "portalextensionapp",
