{
  "$schema": "http://json.schemastore.org/template",
  "author": "Beamable",
  "classifications": [ "beamable", "service" ],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "identity": "Beamable.Templates.BeamService",
  "name": "Beamable Microservice",
  "shortName": "beamservice",
  "sourceName": "BeamService",
  "sources": [
    {
      "modifiers": [
        { "include": [ ".gitignore", ".dockerignore" ] },
        { "exclude": [ ".vs/**", ".template_config/**", "[Content_Types].xml" ,"_rels/**"] }
      ]
    }
  ],
  "symbols": {
    "TemplateVersion": {
      "replaces": "TemplateVersion",
      "type": "bind",
      "binding": "Version"
    },
    "TargetFrameworkOverride": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        { "choice": "net8.0", "description": "Use net8.0 LTS" },
        { "choice": "net9.0", "description": "Use net9.0 STS" },
        { "choice": "net10.0", "description": "Use net10.0 LTS" }
      ],
      "defaultValue": "net10.0",
      "replaces": "TargetFrameworkOverride"
    }
  }
}