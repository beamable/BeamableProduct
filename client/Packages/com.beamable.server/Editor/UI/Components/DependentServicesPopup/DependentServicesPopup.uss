Label {
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
    font-size: 11px;
}

#mainContainer {
    margin-top: 15px;
    margin-bottom: 10px;
    margin-left: 20px;
    margin-right: 20px;
}

#dependencyGridContainer {
    min-width: 580px;
    min-height: 330px;
    flex-direction: column;
    background-color: #A7A7A7;
}

#storageObjectContainer {
    flex-direction: row;
    min-height: 50px;
}

#emptyCornerBox {
    min-width: 125px;
}

#storageObjectsContainer {
    flex-direction: row;
}

#microservicesContainer {
    flex-direction: column;
}

#buttonsContainer {
    flex-direction: row;
    justify-content: flex-end;
    margin-top: 10px;
}

#confirmBtn {
    margin-right: 10px;
    width: 100px;
    height: 28px;
}

#cancelBtn {
    margin-right: 0px;
    margin-left: 0px;
}

.sectionHighlight {
    background-color: #8D8D8D;
}

.sectionHighlightLabel {
    color: #FFFF00;
}
