#mainVisualElement, TemplateContainer, #PublishWindowContainer, PublishPopup, #ContentViewport {
    flex-grow: 1;
    flex-direction: column;
    align-content: stretch;
}
#ContentView {
    left: 0;
    right: 0;
}

#warningMessage{
    margin-top: 15px;
    flex-direction: row;
    align-self: center;
    -unity-font-style: bold;
    font-size: 12px;
    padding-bottom: 15px;
}

#header{
    margin-top: 10px;
    padding-left: 10px;
    padding-right: 40px;
    padding-bottom: 10px;
    -unity-text-align: upper-center;
    flex-direction: row;
    align-content: stretch;
    justify-content: space-between;
}

#nameMS {
    align-self: center;
    margin-left: 3px;
}

#nameC, #depsC {
    flex-direction: row;
    flex: 1;
    margin-right: 10px;
}

#depsC {
    margin-right: 10px;
}

#typeC {
    width: 90px;
}

#enabledC {
    width: 100px;
    justify-content: center;
}
#CommentsC {
    width: 155px;
    padding-left: 20px;
}
#SizeC {
    width: 140px;
}
#commentsText, #SizeC .template{
    width: 80px;
}
#sizeButton Label {
    margin-right: 18px;
    margin-top: 2px;
    font-size: 10px;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
}

.column{
    flex-direction: row;
}
#sizeButton Image#dropDownIcon{
    align-self: flex-end;
    position: absolute;
    top: 6px;
    right: 4px;
    width: 15px;
    height: 7px;
    background-image: resource("Packages/com.beamable/Editor/UI/Content/Icons/dropdown Close.png")
}

.boundedButton {
    height: 20px;
    position: relative;
    top: 0px;
    left: 0px;
    margin-left: 0px;
    margin-right: 10px;
    margin-top: 0px;
    margin-bottom: 0px;
    border-radius: 5px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
    background-image: none;
}
#selectionArea{
    align-self: auto;
    flex-direction: row;
    flex-grow: 1;
    max-width: 120px;
}
#commentsText{
    padding-right: 10px;
    width: 100px;
}
#largeCommentsArea{
    padding-top: 10px;
    padding-right: 0px;
    padding-bottom: 10px;
    padding-left: 0px;
    margin-bottom: 15px;
    margin-left: 0px;
    margin-right: 0px;
    margin-top: 15px;
    min-height: 140px; 
}
#buttonStrip {
    flex-direction: row;
    justify-content: flex-end;
    flex-shrink: 0;
    margin-bottom: 15px;
}
#buttonStrip Button {
    width: 100px;
}

.SubTitleText{
    font-size: 14px;
    -unity-font-style: bold;
}

#manifestsContainer{
    border-color: #000000;
    border-width: 1px;
    padding: 10px;
    min-height: 100px;
    flex-grow: 1;
}

LoadingBarElement {
    align-self: stretch;
    margin-bottom: 5px;
}
#header, #warningMessage, #manifestsContainer, #largeCommentsArea, #buttonStrip{
    margin-left: 15px;
    margin-right: 15px;
}