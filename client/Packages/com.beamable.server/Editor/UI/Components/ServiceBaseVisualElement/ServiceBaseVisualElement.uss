MicroserviceVisualElement > TemplateContainer > #mainVisualElement,
StorageObjectVisualElement > TemplateContainer > #mainVisualElement {
    margin-bottom: 20px;
}

#mainVisualElement {
    flex-direction: row;
}

#serviceCard {
    flex-grow: 1;
}

#foldContainer {
    width: 30px;
    height: 50px;
}

#foldButton {
    height: 30px;
    background-image: none;
    align-items: center;
    justify-content: center;
    background-color: rgba(0, 0, 0, 0);
}

#foldIcon {
    width: 10px;
    height: 10px;
}

#serviceIcon {
    align-self: center;
    margin-left: 4px;
}

#logHeader{
    justify-content: space-between;
    flex-direction: row;
    height: 30px;
    flex-shrink: 0;
}

#logHeader > .leftArea{
    justify-content: center;
    padding-left: 5px;
    flex-direction: row;
    flex-grow: 1;
}

#logHeader > LabeledCheckboxVisualElement,#logHeader > .rightArea {
    padding-left: 0px;
    padding-right: 0px;
    margin-left: 10px;
    margin-right: 10px;
    min-width: 130px;
    max-width: 200px;
    flex-grow: 1;
}

#logHeader > .rightArea{
    flex-direction: row;
    align-content: flex-end;
    justify-content: flex-end;
}
#startBtn {
    width: 28px;
    flex-basis: auto;
    margin-left: 10px;
    margin-right: 10px;
}
#logContainer {
    flex-grow: 1;
}

.logElement {
    flex-grow: 1;
}

.subTitle{
    font-size: 10px;
    -unity-font-style: normal;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
    justify-content: center;
    -unity-text-align: middle-left;
    margin-right: 0px;
    margin-left: 0px;
    margin-top: 0px;
    margin-bottom: 0px;
    padding-right: 0px;
    padding-left: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
    
}

#statusIcon, #remoteStatusIcon{
    /*background-image: resource("Packages/com.beamable.server/Editor/UI/Icons/Local Running L.png");*/
    width: 26px;
    height: 26px;
    align-self: center;
    margin-left: 10px;
    margin-right: 5px;
}


#logText{
    min-height: 100px;
}

#buttonRow{
    align-content: flex-end;
    padding-top: 10px;
    padding-bottom: 10px;
    flex-direction: row;
    justify-content: flex-end;
    flex-shrink: 0;
}

#buildDropDown{
    flex-direction: row;
    justify-content: space-around;
    padding-left: 6px;
    padding-right: 6px;
    padding-top: 0px;
    padding-bottom: 0px;
    border-right-width: 0px;
    font-size: 10px;
}

#buildLabel {
    align-self: center;
}

.confirmBtn{
    font-size: 12px;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-width: 1px;
    border-right-width: 1px;
    margin-right: 0px;
    background-image: none;
    height: 23px;
    cursor: pan;
    background-image: none;
    align-self: center;
}

#start {
    -unity-font-style: bold;
}

.unity-label{
    font-size: 12px;
    -unity-text-align: middle-left;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
}

#microserviceNewTitle {
    -unity-text-align: middle-left;
    margin-top: 3px;
    margin-bottom: 3px;
    flex-grow: 1;
	margin-right: 0px;
}

#moreBtn{
    -unity-slice-bottom: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-left: 0;
    border-right-width: 0px;
    border-top-width: 0px;
    border-bottom-width: 0px;
    border-left-width: 0px;
    padding-left: 11px;
    padding-right: 11px;
    padding-top: 0px;
    padding-bottom: 0px;
    margin-right: 0px;
    margin-left: 0px;
    margin-bottom: 0px;
    margin-top: 0px;
}

LabeledCheckboxVisualElement{
    margin-left: 10px;
	align-self: center;
    justify-content: flex-start;
}

.hide {
    position: absolute;
    left: 999999px;
}

.folded {
    max-height: 50px;
}

.rightArea Button.confirmButton {
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
}

#stopImage, #buildImage {
    align-self: center;
    width: 18px;
    height: 18px;
}

.foldIcon {
    background-image: resource("Packages/com.beamable.server/Editor/UI/Icons/fold_light_dark.png");
}

.unfoldIcon {
    background-image: resource("Packages/com.beamable.server/Editor/UI/Icons/unfold_light_dark.png");
}