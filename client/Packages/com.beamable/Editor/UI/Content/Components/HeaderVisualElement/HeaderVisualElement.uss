HeaderVisualElement {
    justify-content: center;
    height:30px;
}
HeaderVisualElement > TemplateContainer {
    justify-content: center;
}

HeaderVisualElement #mainVisualElement {
    justify-content:flex-end;
}
HeaderVisualElement SplitterVisualElement {
    flex-direction: row;
    align-content: center;
}

HeaderVisualElement SplitterVisualElement > VisualElement {
    border-left-width: 1px;
    border-color: rgba(0,0,0,.1);
    padding-left: 4px;
}

