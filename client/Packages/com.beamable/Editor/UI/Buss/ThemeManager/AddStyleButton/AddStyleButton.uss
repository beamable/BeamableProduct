#addStyleButton {
    margin-top: 10px;
    margin-bottom: 20px;
    padding-top: 3px;
    align-self: center;
    justify-content: center;
    width: 200px;
    height: 26px;
    border-radius: 2px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-color: #000000;
}

#addStyleButton > Label {
    -unity-font-style: bold;
    align-self: center;
}