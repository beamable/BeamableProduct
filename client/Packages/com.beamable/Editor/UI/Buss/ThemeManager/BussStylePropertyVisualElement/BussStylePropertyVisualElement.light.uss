#propertyLabel {
    color:#252525;
}

#propertyLabel:disabled{
    color: #646464;
}

BussStylePropertyVisualElement *:disabled{
    opacity: 1;
    color: #646464;
}

.equalWidth {
    width: 0;
}

BussStylePropertyVisualElement.doesntExists {
    background-color: rgba(255, 255, 255, 0.16);
}
#removeButton {
    background-image: resource("Packages/com.beamable/Editor/UI/Buss/ThemeManager/Assets/remove_Dark.png");
}