#cardHeader {
    color: rgb(37, 37, 37);
}

#colorBlock {
    background-color: rgb(144,144,144);
}

#colorBlock.active {
    background-color: rgb(0,145,255);
}

#sectionHeader {
    color: rgb(37, 37, 37);
    background-color: rgb(180, 180, 180);
}

#sectionLabel {
    color: rgb(37, 37, 37);
}

#properties {
    background-color: rgb(195, 195, 195);
}

#variables {
    background-color: rgb(195, 195, 195);
}

.button {
    background-color: rgb(180, 180, 180);
}

.button > TextElement {
    color: rgb(37, 37, 37);
}

.button:hover {
    background-color: rgb(120,120,120);
}

.headerButton {
    background-color: rgb(180, 180, 180);
}

.headerButton:hover {
    background-color: rgb(162,162,162);
}

.add {
    background-color: rgb(180, 180, 180);
    background-image: resource("Packages/com.beamable.server/Editor/UI/Icons/Add new D.png");
}

.edit {
    background-image: resource("Packages/com.beamable/Editor/UI/Buss/ThemeManager/Assets/editing_dark.png");
}

.wizard {
    background-image: resource("Packages/com.beamable/Editor/UI/Buss/ThemeManager/Assets/wizard_dark.png");
}

.undo {
    background-image: resource("Packages/com.beamable/Editor/UI/Buss/ThemeManager/Assets/undo_dark.png");
}

.cleanAll {
    background-image: resource("Packages/com.beamable/Editor/UI/Buss/ThemeManager/Assets/Erase_dark.png");
}