#mainVisualElement {
    height: 35px;
    flex-direction:row;
}

#leftContentVisualElement {
    flex-direction: row;
    justify-content: flex-start;
    align-items:center;
    margin-right: 10px;
}
#rightContentVisualElement {
    flex-grow: 1;
    justify-content: flex-end;
    flex-direction: row;
    padding-left: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
    min-width: 220px;
}

.boundedButton {
    height: 35px;
    width: 70px;
    position: relative;
    margin-left: 0px;
    margin-right: 0px;
    margin-top: 0px;
    margin-bottom: 0px;
    border-left-width: 0px;
    border-right-width: 1px;
    border-top-width: 0px;
    border-bottom-width: 0px;
    cursor: pan;
    background-image: none;
    border-radius: 0px;
}
.buttonText{
    font-size: 9px;
    -unity-font: url("project:///Packages/com.beamable/Editor/UI/Font/Lato-Semibold.ttf");
    -unity-text-align: middle-center;
    top: 6px;
    margin-right: 2px;
    margin-left: 2px;
}
#contentManagerIcon{
    top: 4px;
    width: 10px;
    height: 12px;
    align-self: center;
}

#skinningIcon{
    top: 4px;
    width: 12px;
    height: 12px;
    align-self: center;
}

#globalConfigIcon{
    top: 4px;
    width: 12px;
    height: 12px;
    align-self: center;
}

#microserviceIcon{
    top: 4px;
    width: 12px;
    height: 12px;
    align-self: center;
}

.unboundedButton {
    height: 35px;
    width: 38px;
    position: relative;
    align-items: center;
    margin-left: 0px;
    margin-right: 0px;
    margin-top: 0px;
    margin-bottom: 0px;
    border-left-width: 1px;
    border-right-width: 0px;
    border-top-width: 0px;
    border-bottom-width: 1px;
    padding-left: 0px;
    padding-right: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
    background-image: none;
    border-radius: 0px;
    cursor: pan;
}

#categoryIcon{
    top: 5px;
    width: 12px;
    height: 12px;
    align-self: center;
}

#typeIcon{
    top: 5px;
    width: 12px;
    height:12px;
    align-self: center;
}

#infoIcon{
    top: 8px;
    width: 8px;
    height: 15px;
    align-self: center;
}

#searchBar {
    align-self: center;
    border-radius: 8px;
    flex-grow: 1;
}

Button.unity-button,
Button {
    background-color: rgba(0,0,0,0);
}

#accountIcon{
    top: 8px;
    width: 15px;
    height: 15px;
    align-self: center;
}
