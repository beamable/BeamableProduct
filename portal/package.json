{
  "name": "portal",
  "version": "1.0.0",
  "description": "admin application for beamable",
  "keywords": [],
  "main": "src/index.ts",
  "author": "Beamable",
  "scripts": {
    "cleanup": "rimraf ./dist",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "start": "npm run cleanup && cross-env NODE_ENV=development rollup --config --watch",
    "start:staging": "npm run cleanup && cross-env NODE_ENV=development PORTAL_ENV=staging rollup --config --watch",
    "start:prod": "npm run cleanup && cross-env NODE_ENV=development PORTAL_ENV=prod rollup --config --watch",
    "build": "npm run cleanup && cross-env NODE_ENV=development rollup --config",
    "build:release": "npm run cleanup && npm run test && cross-env NODE_ENV=production rollup --config",
    "build:release:dev": "npm run cleanup && cross-env NODE_ENV=production PORTAL_ENV=dev rollup --config",
    "build:release:staging": "npm run cleanup && cross-env NODE_ENV=production PORTAL_ENV=staging rollup --config",
    "build:release:prod": "npm run cleanup && cross-env NODE_ENV=production PORTAL_ENV=prod rollup --config"
  },
